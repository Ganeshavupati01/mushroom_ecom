{
  "version": 2,
  "builds": [
    {
      "src": "../client/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "../client/$1"
    }
  ]
}
